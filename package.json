{
  "name": "find-node-modules-import",
  "version": "1.0.1",
  "description": "Find specific node modules import statement in your source code",
  "type": "module",
  "keywords": [
    "npm",
    "node",
    "import",
    "analytics",
    "core-modules",
    "builtin"
  ],
  "homepage": "https://github.com/azu/find-node-modules-import",
  "bugs": {
    "url": "https://github.com/azu/find-node-modules-import/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/find-node-modules-import.git"
  },
  "license": "MIT",
  "author": "azu",
  "sideEffects": false,
  "main": "lib/find-node-modules-import.js",
  "types": "lib/find-node-modules-import.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "bin": {
    "find-node-modules-import": "bin/cmd.mjs"
  },
  "files": [
    "bin/",
    "lib/",
    "module/",
    "src/"
  ],
  "scripts": {
    "build": "tsc -p .",
    "clean": "rimraf lib/ module/",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "lint-staged": "^13.1.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.2",
    "rimraf": "^4.0.4",
    "ts-node": "^10.9.1",
    "ts-node-test-register": "^10.0.0",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@1.22.18",
  "dependencies": {
    "builtin-modules": "^3.3.0",
    "es-module-lexer": "^1.1.0",
    "globby": "^13.1.3",
    "meow": "^11.0.0",
    "structured-source": "^4.0.0"
  }
}
